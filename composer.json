{
    "name": "shiguangxiaotou3/myweb",
    "description": "基于yii2框架的二次开发",
    "keywords": ["yii2", "二次开发", "myweb", "project template"],
    "homepage": "http://www.shiguangxiaotou.com",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "chenkby/yii2-region": "dev-master",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "phpoffice/phpspreadsheet": "^1.20",
        "practically/yii2-chartjs": "^1.1",
        "ipinfo/ipinfo": "^2.3",
        "mdmsoft/yii2-admin": "~2.0",
        "abhi1693/yii2-system-info": "@dev",
        "ddeboer/imap": "^1.12",
        "eluhr/yii2-aceeditor": "^0.0.6",
        "alibabacloud/alidns-20150109": "2.0.1",
        "oonne/yii2-webuploader": "*",
        "kartik-v/yii2-ipinfo": "dev-master",
        "kartik-v/yii2-widget-alert": "dev-master",
        "kartik-v/yii2-widget-growl": "dev-master",
        "yiisoft/yii2-bootstrap" :"@dev",
        "froala/yii2-froala-editor": "^3.2.5",
        "newerton/yii2-fancybox": "^1.0",
        "mludvik/yii2-tags-input": "~1.0",
        "himiklab/yii2-handsontable-widget": "*",
        "johnitvn/yii2-ajaxcrud": "*",
        "markrogoyski/math-php": "2.*",
        "alibabacloud/dysmsapi": "^1.8",
        "imangazaliev/didom": "^1.18",
        "wechatpay/wechatpay": "^1.4"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
