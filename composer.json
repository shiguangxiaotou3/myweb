{
    "name": "shiguangxiaotou3/myweb",
    "description": "基于yii2框架的二次开发",
    "keywords": ["yii2", "二次开发", "myweb", "project template"],
    "homepage": "http://www.shiguangxiaotou.com",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-typeahead": "dev-master",
        "kartik-v/yii2-widget-fileinput": "dev-master",
        "johnitvn/yii2-ajaxcrud": "*",
        "chenkby/yii2-region": "dev-master",
        "phpoffice/phpspreadsheet": "^1.20",
        "practically/yii2-chartjs": "^1.1",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "ipinfo/ipinfo": "^2.3",
        "mdmsoft/yii2-admin": "~2.0",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
